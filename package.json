{
  "name": "final-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "backend-start": "export NODE_ENV=production && node ./bin/www",
    "backend-dev": "export NODE_ENV=development && nodemon ./bin/www",
    "frontend-start": "npm start --prefix frontend",
    "frontend-build": "npm run build --prefix frontend",
    "back-front-prod": "concurrently \"npm run backend-start\" \"npm run frontend-build\"",
    "back-front-dev": "concurrently \"npm run backend-dev\" \"npm run frontend-start\"",
    "eslint": "eslint . --ignore-path .gitignore --ext .js || exit 0",
    "heroku-postbuild": "cd frontend && npm install && npm run build"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.5.2",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "helmet": "^3.21.2",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.8.11",
    "mongoose-autopopulate": "^0.12.0",
    "mongoose-sequence": "^5.2.2",
    "morgan": "~1.9.1",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.2",
    "nodemon": "^2.0.2",
    "npm": "^6.13.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "uuid-token-generator": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-security": "^1.4.0"
  }
}
